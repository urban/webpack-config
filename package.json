{
  "name": "@urban/webpack-config",
  "version": "1.3.2",
  "description": "An opinionated webpack config.",
  "author": "Urban Faubion",
  "license": "MIT",
  "homepage": "https://github.com/urban/webpack-config#readme",
  "keywords": [
    "config",
    "webpack",
    "ES6",
    "ES2015"
  ],
  "bugs": {
    "url": "https://github.com/urban/webpack-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urban/webpack-config.git"
  },
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "LICENSE",
    "src"
  ],
  "scripts": {
    "build:lib": "rimraf lib && cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "rimraf lib && cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:lib && npm run build:es",
    "check": "npm run test:lint && npm test && npm run test:audit && npm outdated --depth 0",
    "clean": "rimraf lib dist coverage",
    "prepublish": "npm run build",
    "test:audit": "nsp check",
    "test:coverage": "rimraf coverage && cross-env NODE_ENV=test babel-node $(npm bin)/isparta cover test",
    "test:lint": "standard",
    "test:typecheck": "flow check ./src",
    "test": "cross-env NODE_ENV=test babel-node $(npm bin)/tape test | tap-spec",
    "preversion": "npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@urban/promisify": "1.0.3",
    "autoprefixer": "6.3.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "json-loader": "0.5.4",
    "nsp": "2.2.0",
    "postcss-loader": "0.8.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "rimraf": "2.5.1",
    "standard": "6.0.4",
    "style-loader": "0.13.0",
    "tap-spec": "4.1.1",
    "tape": "4.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.13",
    "webpack-merge": "0.7.3"
  },
  "peerDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-loader": "^6.2.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.0.0 || 2.0.7-beta || ^2.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "examples/*/public/",
      "interfaces/*"
    ]
  }
}
