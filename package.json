{
  "name": "@urban/webpack-config",
  "version": "1.2.0",
  "description": "An opinionated webpack config.",
  "author": "Urban Faubion",
  "license": "MIT",
  "homepage": "https://github.com/urban/webpack-config#readme",
  "keywords": [
    "config",
    "webpack",
    "ES6",
    "ES2015"
  ],
  "bugs": {
    "url": "https://github.com/urban/webpack-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urban/webpack-config.git"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "build": "rimraf lib && npm run build:lib",
    "check": "npm run test:lint && npm test && npm run test:audit && npm outdated --depth 0",
    "prepublish": "npm run build",
    "test:audit": "nsp check",
    "test:coverage": "rimraf coverage && babel-node $(npm bin)/isparta cover ./test/*-test.js",
    "test:lint": "standard",
    "test:typecheck": "flow check ./src",
    "test": "NODE_ENV=test babel-node $(npm bin)/tape ./test/*-test.js | tap-spec",
    "preversion": "npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "invariant": "^2.2.0",
    "lodash.merge": "^3.3.2",
    "lodash.reduce": "3.1.2"
  },
  "devDependencies": {
    "@urban/promisify": "^1.0.3",
    "autoprefixer": "6.1.2",
    "babel-cli": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "json-loader": "^0.5.4",
    "nsp": "^2.1.0",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.4.4",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "peerDependencies": {
    "autoprefixer": "*",
    "babel-loader": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "json-loader": "*",
    "postcss-color-function": "*",
    "postcss-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "*"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "examples/*/public/",
      "interfaces/*"
    ]
  }
}
